{
  "name": "neon/site",
  "description": "Neon CMS' website handler.",
  "version": "3.0.0-alpha-1",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bal√°zs Ercsey",
      "email": "balazs.ercsey@elementary-interactive.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Neon\\Site\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neon\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Neon\\Site\\NeonSiteServiceProvider"
      ],
      "aliases": {
        "Site": "Neon\\Site\\Facades\\Site"
      }
    }
  },
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "neon/model-uuid": "^1.0",
    "neon/attributable": "*",
    "spatie/laravel-package-tools": "^1.14.0",
    "illuminate/contracts": "^10.0"
  },
  "repositories": [],
  "branch-alias": {},
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": false,
      "phpstan/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": false
}
