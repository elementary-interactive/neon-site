{
  "name": "neon/site",
  "description": "Neon CMS' website handler.",
  "version": "2.0.5",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bal√°zs Ercsey",
      "email": "balazs.ercsey@elementary-interactive.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Neon\\Site\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neon\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Neon\\Site\\NeonSiteServiceProvider"
      ],
      "aliases": {
        "Site": "Neon\\Site\\Facades\\Site"
      }
    }
  },
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "neon/model-uuid": "^1.0",
    "neon/attributable": "*",
    "spatie/laravel-package-tools": "^1.14.0",
    "illuminate/contracts": "^10.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^7.8",
    "larastan/larastan": "^2.0.1",
    "orchestra/testbench": "^8.8",
    "pestphp/pest": "^2.20",
    "pestphp/pest-plugin-arch": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "spatie/laravel-ray": "^1.26"
  },
  "repositories": [],
  "branch-alias": {},
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
